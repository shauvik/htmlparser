      <directory>lexer/src</directory>
      <outputDirectory>lexer/src</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>filterbuilder</directory>
      <outputDirectory>filterbuilder</outputDirectory>
      <includes>
        <include>pom.xml</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>filterbuilder/src</directory>
      <outputDirectory>filterbuilder/src</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>sitecapturer</directory>
      <outputDirectory>sitecapturer</outputDirectory>
      <includes>
        <include>pom.xml</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>sitecapturer/src</directory>
      <outputDirectory>sitecapturer/src</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>thumbelina</directory>
      <outputDirectory>thumbelina</outputDirectory>
      <includes>
        <include>pom.xml</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>thumbelina/src</directory>
      <outputDirectory>thumbelina/src</outputDirectory>
    </fileSet>
  </fileSets>
</assembly>